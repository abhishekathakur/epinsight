<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.epinsight" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EPInsight</name>
    <description>
        A Simple Dashboard App.
    </description>
    <author email="abhishekthakur001@hoo.co.in" href="">
        Developed By IT Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>


	
	<access origin="http://127.0.0.1*"/> <!-- allow local pages -->

    <!-- <access origin="https://example.com" /> allow any secure requests to example.com -->
    <!-- <access origin="https://example.com" subdomains="true" /> such as above, but including subdomains, such as www -->
    <access origin=".*"/>
    <access origin="http://10.0.2.2"/>
	<access origin="http://192.168.10.142*"/>
	<access origin="http://192.168.1.6*"/>
    <access origin="http://10.0.2.2*"/>


</widget>
